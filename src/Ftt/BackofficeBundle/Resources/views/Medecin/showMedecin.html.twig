{% extends 'FttBackofficeBundle::accueil_admin.html.twig' %}

{% block content %}


    <div class="box box-info">
        <div class="box-header with-border">
            <h3 class="box-title">Details Medecin</h3>
        </div>



        <div class="box-body">

            <div class="form-group">
                <dt> Nom :</dt>{{ Medecin.nom }}
            </div>
            <hr>

            <div class="form-group">
                <dt> Prenom :</dt>{{ Medecin.prenom }}
            </div>
            <hr>



        </div><!-- /.box-body -->

        <div class="box-footer">
            <a class="btn btn-primary btn-block" href="{{ path('ftt_backoffice_medecin_list') }}">Retourner</a>
            <a class="btn btn-primary btn-block"
               href="{{ path('ftt_backoffice_medecin_update', { 'id': Medecin.id }) }}">Modifier</a>

        </div><!-- /.box-footer -->
    </div>


{% endblock %}

